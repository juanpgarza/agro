        -->
        
        <!-- me permite filtrar los cuadros del tablero por despósito -->
        <record id="view_pickingtype_filter_inherited" model="ir.ui.view">
            <field name="name">Agro</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_pickingtype_filter"/>
            <field name="arch" type="xml">
                <field name="warehouse_id" position="after">
                    <filter name="filter_mi_deposito" string="Mi depósito" domain="[('warehouse_id_id','=',True)]" />
                    <separator/>			
                    <filter name="pickingtype_filter_GR" string="Dep. Roca" domain="[('warehouse','ilike','%roca%')]" />
                    <filter name="pickingtype_filter_VR" string="Dep. Regina" domain="[('warehouse','ilike','%regina%')]" />
                </field>
            </field>
        </record>

        <record id="stock.stock_picking_type_action" model="ir.actions.act_window">
            <field name="context">{'search_default_filter_mi_deposito': 1}</field>
        </record>

